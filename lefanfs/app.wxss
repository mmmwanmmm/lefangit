/**app.wxss**/
@import 'style/weui.wxss';
page{
    background-color: #EFEFF4;
    font-size: 14px;
     font-family:"Open Sans","Helvetica Neue",Helvetica,Arial,"Microsoft Yahei",sans-serif;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.page__hd {
    padding: 40px;
}
.page__bd {
    padding-bottom: 40px;
}
.page__bd_spacing {
    padding-left: 15px;
    padding-right: 15px;
}

.page__ft{
    padding-bottom: 10px;
    text-align: center;
}

.page__title {
    text-align: left;
    font-size: 20px;
    font-weight: 400;
}

.page__desc {
    margin-top: 5px;
    color: #888888;
    text-align: left;
    font-size: 14px;
}
.btn {
  width: 90%;
  padding: 25rpx 0;
  border-radius: 10rpx;
  text-align: center;
}
.lefan-login-btn {
  margin: 40rpx 5%;
  border-radius: 10rpx;
  background: #20bb21;
  color: #fff;
}
.loading_box {
  width: 100%;
  text-align: center;
  padding: 100rpx 0;
}

.loading_box>view {
  display: inline-block;
}
.loading1 {
  background: url('data:image/svg+xml;base64,') no-repeat;
  background-size: cover;
  width: 50rpx;
  height: 50rpx;
  -webkit-animation: loading 1s steps(12, end) infinite;
  animation: loading 1s steps(12, end) infinite;
}

@-webkit-keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}

@keyframes loading {
  0% {
    -webkit-transform: rotate3d(0, 0, 1, 0deg);
    transform: rotate3d(0, 0, 1, 0deg);
  }

  100% {
    -webkit-transform: rotate3d(0, 0, 1, 360deg);
    transform: rotate3d(0, 0, 1, 360deg);
  }
}