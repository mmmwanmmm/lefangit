package com.lefanfs.base.dto;

import java.io.Serializable;

public class IdNameDto implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -5006497097394252415L;

	private Integer id;
	private String name;

	public IdNameDto() {

	}

	public IdNameDto(Integer _id, String _name) {
		this.id = _id;
		this.name = _name;
	}

	public Integer getId() {
		return id;
	}

	public void setId(Integer id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

}
